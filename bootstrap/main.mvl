include "../std/io.mvl"

proc main argc: s64 argv: s64
  if argc <= 1
    @println "ERROR: output and input files were not provided"
    @exit 1
  end

  if argc <= 2
    @println "ERROR: input file was not provided"
     @exit 1
  end

  ptr = !add argv 8
  output_path = *s64 ptr
  ptr = !add argv 16
  input_path = *s64 ptr

  @print "Input file path: "
  @println input_path
  @print "Output file path: "
  @println output_path
